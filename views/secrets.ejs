    <script>
      var header = document.getElementById("header");
      var ms = document.getElementById("models");
      var mz = document.getElementById("modelz");
      var my = document.getElementById("modely");
      var mx = document.getElementById("modelx");
      var model = document.getElementById("model");
      var car = document.getElementById("car");
      var car1 = document.getElementById("car1");
      var car2 = document.getElementById("car2");


      ms.onclick = function(){
        header.style.backgroundImage = "url(/images/image-1.jpeg)";
        model.innerHTML = "Model S"
        car.innerHTML = "1.9 s"
        car1.innerHTML = "200 mph"
        car2.innerHTML = "396 mi"
      }
      mz.onclick = function () {
        header.style.backgroundImage = "url(/images/image-2.png)";
        model.innerHTML = "Model Z"
        car.innerHTML = "1.7"
        car1.innerHTML = "250 mph"
        car2.innerHTML = "308 mi"
      }
      my.onclick = function () {
        header.style.backgroundImage = "url(/images/image-3.png)";
        model.innerHTML = "Model Y"
        car.innerHTML = "1.6"
        car1.innerHTML = "300 mph"
        car2.innerHTML = "250 mi"
      }
      mx.onclick = function () {
        header.style.backgroundImage = "url(/images/image-4.png)";
        model.innerHTML = "Model X"
        car.innerHTML = "1.8"
        car1.innerHTML = "280 mph"
        car2.innerHTML = "400 mi"
      }
    </script>

    



    
